local Tab = Window:MakeTab({
	Name = "Misc (Anim)",
	Icon = "rbxassetid://7734111084",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Miscellaneous."
})

Tab:AddTextbox({
	Name = "WalkSpeed Textbox",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		getgenv().WalkSpeedValue = Value; --set your desired walkspeed here
local Player = game:service'Players'.LocalPlayer;
Player.Character.Humanoid:GetPropertyChangedSignal'WalkSpeed':Connect(function()
Player.Character.Humanoid.WalkSpeed = getgenv().WalkSpeedValue;
end)
Player.Character.Humanoid.WalkSpeed = getgenv().WalkSpeedValue;
	end	  
})

Tab:AddTextbox({
	Name = "JumpPower Textbox",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		getgenv().JumpPowerValue = Value; --set your desired walkspeed here
local Player = game:service'Players'.LocalPlayer;
Player.Character.Humanoid:GetPropertyChangedSignal'JumpPower':Connect(function()
Player.Character.Humanoid.JumpPower = getgenv().JumpPowerValue;
end)
Player.Character.Humanoid.JumpPower = getgenv().JumpPowerValue;
	end	  
})

Tab:AddTextbox({
	Name = "HipHeight (Default = 0)",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.HipHeight = (Value)
	end	  
})

Tab:AddTextbox({
	Name = "Gravity (Default = 196.2)",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		game.Workspace.Gravity = (Value)
	end	  
})

Tab:AddTextbox({
	Name = "Brightness (Default = 1)",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		game.Lighting.Brightness = Value
	end	  
})

