        Animate.Disabled = true
        StopAnim()
        Animate.climb.ClimbAnim.AnimationId = "http://www.roblox.com/asset/?id=1148811837"
        plr.Character.Humanoid:ChangeState(3)
	Animate.Disabled = false
  	end    
})

